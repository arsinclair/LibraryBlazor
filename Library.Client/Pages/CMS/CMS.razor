@page "/cms/"
@inject NavigationManager NavigationManager

@* @DynamicComponent *@

<button class="btn btn-primary rounded-0" @onclick="@(e => NavigateTo("/cms/settings/entities"))">Settings</button>

@code {
    public ComponentBase DynamicComponent {get; set;}

    private void NavigateTo(string url) 
    {
        NavigationManager.NavigateTo(url);
    }
}